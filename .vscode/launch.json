{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "Travel Agent: Paris",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/travel_agent.py",
      "args": ["Paris", "--output", "paris_travel_plan.json"],
      "console": "integratedTerminal"
    },
    {
      "name": "Streamlit: Travel Agent UI",
      "type": "python",
      "request": "launch",
      "module": "streamlit",
      "args": ["run", "${workspaceFolder}/chat.py"],
      "console": "integratedTerminal"
    }
  ]
}
